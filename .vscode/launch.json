    "version": "0.2.0",
    "configurations": [

      
        {
            "command": "npm start",
            
            
            "type": "pwa-node",
            "request": "launch",
            "name": "Run Slack-onboard bot",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "-r", "ts-node/register", "src/index.ts"
            ],
            "envFile": "${workspaceFolder}/.env",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal"
        },

        
        
    ]
}